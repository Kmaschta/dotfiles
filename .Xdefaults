! Extensions
URxvt.perl-lib: ~/.urxvt/ext/
URxvt*.perl-ext-common: default,matcher,tabbed,clipboard

! Style
URxvt*.scrollBar: false
URxvt*.background: Black
URxvt*.foreground: rgb:20/64/22
URxvt*.font: xft:bitstream vera sans mono:medium:pixelsize=18,style=bold
URxvt*.letterSpace: -1

! Big mode
URxvt*.foreground: white
URxvt*.font: xft:bitstream vera sans mono:medium:pixelsize=25,style=bold

!! Xft
Xft.dpi:        141
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull


! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

! Url matcher
URxvt*.url-launcher: /usr/bin/chromium-browser
URxvt*.matcher.button: 1
URxvt*.keysym.C-Delete: perl:matcher:last
URxvt*.keysym.M-Delete: perl:matcher:list
URxvt*.colorUL: #4682B4

! Copy / Paste
URxvt*.iso14755: False
URxvt*.keysym.Shift-Control-C: perl:clipboard:copy
URxvt*.keysym.Shift-Control-V: perl:clipboard:paste
